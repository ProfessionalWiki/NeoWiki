{
	"extends": [
		"stylelint-config-wikimedia"
	],
	"plugins": [
		"stylelint-scss"
	],
	"rules": {
		"at-rule-no-unknown": null,
		"scss/at-rule-no-unknown": true,
		"declaration-empty-line-before": null,
		"font-family-name-quotes": "always-where-recommended",
		"declaration-no-important": null
	}
}
