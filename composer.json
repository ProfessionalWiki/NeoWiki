{
	"name": "professional-wiki/neowiki",
	"type": "mediawiki-extension",
	"license": "proprietary",
	"description": "TODO",
	"authors": [
		{
			"name": "Professional.Wiki",
			"email": "info@professional.wiki",
			"homepage": "https://Professional.Wiki"
		},
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "https://www.EntropyWins.wtf/mediawiki"
		}
	],
	"require": {
		"php": "^8.3",
		"composer/installers": "^2|^1.0.1",
		"professional-wiki/neo": "@dev",
		"opis/json-schema": "^2.3.0",
		"laudis/neo4j-php-client": "^3.1.2",
		"twig/twig": "^3.5.1",
		"jeroen/psr-log-test-doubles": "^3.0.0",
		"jeroen/file-fetcher": "^6.1.0"
	},
	"require-dev": {
		"vimeo/psalm": "^5.25.0",
		"phpstan/phpstan": "^1.12.2",
		"mediawiki/mediawiki-codesniffer": "44.0.0"
	},
	"autoload": {
		"psr-4": {
			"ProfessionalWiki\\NeoWiki\\MediaWiki\\": "src/",
			"ProfessionalWiki\\NeoWiki\\": "Neo/src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ProfessionalWiki\\NeoWiki\\Tests\\MediaWiki\\": "tests/phpunit/",
			"ProfessionalWiki\\NeoWiki\\Tests\\": "Neo/tests/"
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"php-http/discovery": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"extra": {
		"installer-name": "NeoWiki"
	},
	"repositories": [
		{
			"type": "path",
			"url": "Neo"
		}
	]
}
